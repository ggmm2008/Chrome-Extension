{
  "name": "极客头条 - CSDN",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "发现、分享值得你关注的极客新闻。",
  "homepage_url": "https://github.com/jzhangdev/Geek-CSDN-Chrome-Extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "极客头条",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "contentSettings",
    "cookies",
    "http://geek.csdn.net/"
  ]
}